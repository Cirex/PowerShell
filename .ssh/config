SendEnv LANG LC_* GIT_*
KeepAlive yes
ServerAliveInterval 60

Host devel
  Hostname devel.cirex.us
  User cirex

Host fenix
  Hostname fenix-novus.com
  User fenix

Host github.com
  HostName github.com
  User git
  IdentityFile ~/Keys/GitHub.key

Host bitbucket.org
  HostName bitbucket.org
  User git
  IdentityFile ~/Keys/BitBucket.key

Host heroku.com
  HostName heroku.com
  User git
  IdentityFile ~/Keys/Heroku.key